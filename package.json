{
  "name": "js-payments",
  "version": "0.0.0",
  "description": "> A JavaScript interface for allowing tokenisation and authorisation of payments through Trust Payments.",
  "typings": "./dist/index.d.ts",
  "private": true,
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Safari versions",
    "last 1 Opera version",
    "ie 11",
    "edge 18",
    "iOS 10-14",
    "safari 12-14",
    "Android 7-10"
  ],
  "config": {
    "host": "localhost",
    "locale": "en_GB"
  },
  "engines": {
    "node": "16.13.1",
    "npm": "8.3.0"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.cjs && npm run build:declarations",
    "build:prod": "webpack --config webpack.prod.cjs && npm run build:declarations",
    "build:mock": "webpack --config webpack.mock.cjs && npm run build:declarations",
    "build:dev": "webpack --config webpack.dev.cjs && npm run build:declarations",
    "build:rc": "webpack --config webpack.rc.cjs && npm run build:declarations",
    "prod": "npm run build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "coverage": "jest --coverage",
    "typescript:check": "npx tsc --noEmit -p tsconfig.json",
    "build:declarations": "npx dts-bundle-generator ./src/declarations.ts -o ./dist/index.d.ts --project tsconfig.json --no-banner",
    "testdocker": "docker pull \"securetrading1/js-payments-testing:$DOCKER_BRANCH\" && docker run -d -p 8443:8443 -p 8760:8760 -it \"securetrading1/js-payments-testing:$DOCKER_BRANCH\"",
    "behave": "npm run testdocker && cd js-payments-testing &&",
    "export:translations": "cross-env-shell i18next-conv -l '$npm_package_config_locale' -s src/application/core/translations/$npm_package_config_locale.json -t src/translations/$npm_package_config_locale.po",
    "import:translations": "cross-env-shell i18next-conv -l '$npm_package_config_locale' -s src/translations/$npm_package_config_locale.po -t src/application/core/translations/$npm_package_config_locale.json",
    "preinstall": "npx npm-force-resolutions",
    "prettier": "npx prettier --write \"./src/**/*.{ts,js,css,json}\"",
    "example": "ws --rewrite '/* -> /dist/$1' --https",
    "start:automated": "webpack serve --color --progress --config webpack.test.js",
    "start:bundle": "webpack serve --color --progress --config webpack.dev.cjs",
    "example:html:dev": "npm run start --prefix ./example/html",
    "example:html:start": "run-p start:bundle example:html:dev",
    "start": "npm run example:html:start",
    "example:react:dev": "npm run start --prefix ./example/reactjs",
    "example:react:start": "run-p start:bundle example:react:dev",
    "example:angular:dev": "npm run start --prefix ./example/angular",
    "example:angular:start": "run-p start:bundle example:angular:dev",
    "example:vue:dev": "npm run start --prefix ./example/vuejs",
    "example:vue:start": "run-p start:bundle example:vue:dev",
    "lint": "npx eslint ./src",
    "lint:fix": "npx eslint --fix ./src",
    "prepare": "husky install",
    "translations:missing": "./bin/translations.js missing",
    "translations:coverage": "./bin/translations.js coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/trustpayments-public/stjs/js-payments.git"
  },
  "author": "",
  "type": "module",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/trustpayments-public/stjs/js-payments/-/issues"
