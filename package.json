{
  "name": "js-payments",
  "version": "0.0.0",
  "description": "> A JavaScript interface for allowing tokenisation and authorisation of payments through Trust Payments.",
  "private": true,
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "last 2 Safari versions",
    "last 1 Opera version",
    "ie 11",
    "edge 18",
    "iOS 10-14",
    "safari 12-14",
    "Android 7-10"
  ],
  "config": {
    "host": "localhost",
    "locale": "en_GB"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:automated": "webpack --config webpack.test.js",
    "build:dev": "webpack --config webpack.dev.js",
    "build:rc": "webpack --config webpack.rc.js",
    "prod": "npm run build",
    "test": "jest --watchAll",
    "coverage": "jest --coverage",
    "snyk": "snyk test --org=secure-trading",
    "testdocker": "docker pull \"securetrading1/js-payments-testing:$DOCKER_BRANCH\" && docker run -d -p 8443:8443 -p 8760:8760 -it \"securetrading1/js-payments-testing:$DOCKER_BRANCH\"",
    "behave": "npm run testdocker && cd js-payments-testing &&",
    "export:translations": "cross-env-shell i18next-conv -l '$npm_package_config_locale' -s src/application/core/translations/$npm_package_config_locale.json -t src/translations/$npm_package_config_locale.po",
    "import:translations": "cross-env-shell i18next-conv -l '$npm_package_config_locale' -s src/translations/$npm_package_config_locale.po -t src/application/core/translations/$npm_package_config_locale.json",
    "snyk-protect": "snyk protect",
    "snyk-auth": "snyk auth $SNYK_API_TOKEN",
    "postshrinkwrap": "npx npm-force-resolutions && npm install --ignore-scripts",
    "prettier": "./node_modules/.bin/prettier --write ./src/**/*.{ts,js,css,json}",
    "example": "ws --rewrite '/* -> /dist/$1' --https",
    "start:automated": "webpack-dev-server --color --progress --config webpack.test.js",
    "start:bundle": "webpack-dev-server --color --progress --config webpack.dev.js",
    "example:html:dev": "npm run start --prefix ./example/html",
    "example:html:start": "run-p start:bundle example:html:dev",
    "start": "npm run example:html:start",
    "example:react:dev": "npm run start --prefix ./example/reactjs",
    "example:react:start": "run-p start:bundle example:react:dev",
    "example:angular:dev": "npm run start --prefix ./example/angular",
    "example:angular:start": "run-p start:bundle example:angular:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/trustpayments-public/stjs/js-payments.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/trustpayments-public/stjs/js-payments/-/issues"
  },
  "homepage": "https://gitlab.com/trustpayments-public/stjs/js-payments/-/blob/master/README.md",
  "resolutions": {
    "ajv": "6.12.5",
    "dot-prop": "5.3.0",
    "minimist": "1.2.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run prettier"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/plugin-proposal-optional-catch-binding": "7.10.4",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/plugin-transform-typescript": "7.11.0",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-typescript": "7.10.4",
    "@types/hapi__joi": "17.1.6",
    "@types/jest": "26.0.14",
    "@types/jest-each": "24.3.0",
    "@types/node": "14.11.2",
    "babel-jest": "26.5.2",
    "babel-loader": "8.1.0",
    "babel-plugin-transform-typescript-metadata": "0.3.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.2.0",
    "coveralls": "3.1.0",
    "cross-env": "7.0.2",
    "css-loader": "4.3.0",
    "cssnano": "4.1.10",
    "file-loader": "6.1.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-loader": "1.3.1",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "i18next-conv": "10.0.2",
    "jest": "26.5.3",
    "jest-each": "26.5.2",
    "jest-fetch-mock": "3.0.3",
    "jest-get-type": "26.3.0",
    "lint-staged": "10.4.0",
    "live-server": "1.2.1",
    "local-web-server": "4.2.1",
    "mini-css-extract-plugin": "1.0.0",
    "node-sass": "4.14.1",
    "npm-run-all": "4.1.5",
    "postcss-import": "12.0.1",
    "postcss-loader": "4.0.4",
    "postcss-preset-env": "6.7.0",
    "postcss-scss": "3.0.2",
    "prettier": "2.1.1",
    "sass-loader": "10.0.2",
    "snyk": "1.409.1",
    "source-map-loader": "1.1.1",
    "style-loader": "1.3.0",
    "stylelint-webpack-plugin": "2.1.0",
    "ts-jest": "26.4.1",
    "ts-loader": "8.0.4",
    "ts-mockito": "2.6.1",
    "tslint": "6.1.3",
    "tslint-loader": "3.5.4",
    "typescript": "4.0.3",
    "url-loader": "4.1.1",
    "webpack": "4.44.1",
    "webpack-bundle-analyzer": "3.9.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-manifest-plugin": "2.2.0",
    "webpack-merge": "5.2.0"
  },
  "dependencies": {
    "@babel/polyfill": "7.11.5",
    "@babel/runtime": "7.11.2",
    "@securetrading/js-payments-card": "1.0.23",
    "@sentry/browser": "5.24.2",
    "@sheerun/mutationobserver-shim": "0.3.3",
    "@trustpayments/jwt-generator": "0.1.11",
    "@trustpayments/ts-iin-lookup": "0.1.10",
    "@trustpayments/ts-luhn-check": "0.1.9",
    "@types/browserslist": "4.8.0",
    "@types/jwt-decode": "2.2.1",
    "@types/lodash": "4.14.161",
    "@types/redux-logger": "3.0.8",
    "bowser": "2.11.0",
    "browserslist": "4.14.5",
    "core-js": "2.6.11",
    "fastestsmallesttextencoderdecoder": "1.0.22",
    "i18next": "19.8.3",
    "joi": "17.2.1",
    "jwt-decode": "3.0.0",
    "location-origin": "1.1.4",
    "lodash": "4.17.20",
    "minimist": "1.2.5",
    "npm-force-resolutions": "0.0.3",
