  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --host=0.0.0.0 --https --open -p 3000 --key-file ./src/cert/key.pem --cert-file ./src/cert/cert.pem",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "run-s build develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "docker": "docker-compose up -d",
