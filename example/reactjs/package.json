    "prettier": "2.6.2"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop --host=0.0.0.0 --https --open -p 3000 --key-file ./src/cert/key.pem --cert-file ./src/cert/cert.pem",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "run-s build develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "docker": "docker-compose up -d",
    "docker:build": "docker build -t securetrading1/reactjs-payments .",
    "docker:build:test": "docker build -t securetrading1/reactjs-payments --build-arg environment=test ."
  }
