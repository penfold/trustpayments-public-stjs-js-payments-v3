{
  "name": "reactnative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "node-scripts": "cd scripts/ && node updateJwt.js && node copyLibrary.js",
    "android-apk": "npm run node-scripts && cd android/ && gradlew assembleRelease && npm run android-apk-message",
    "android-apk-message": "echo Apk location: /android/app/build/outputs/apk/",
    "android": "npm run node-scripts && react-native run-android",
    "ios": "npm run node-scripts && react-native run-ios",
    "start": "npm run node-scripts && react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "react": "17.0.2",
    "react-native": "0.64.0",
    "react-native-webview": "11.4.2",
    "@trustpayments/jwt-generator": "0.1.48"
  },
  "devDependencies": {
    "@babel/core": "7.13.16",
    "@babel/runtime": "7.13.17",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/jest": "26.0.22",
    "@types/react-native": "0.64.4",
    "@types/react-test-renderer": "17.0.1",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "babel-jest": "26.6.3",
    "eslint": "7.24.0",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.66.0",
    "react-test-renderer": "17.0.2",
    "typescript": "4.2.4"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
