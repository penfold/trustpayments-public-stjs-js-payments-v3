{
  "name": "reactnative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "node-scripts": "cd scripts/ && node updateJwt.js && node copyLibrary.js",
    "android-apk": "npm run node-scripts && cd android/ && gradlew assembleRelease && npm run android-apk-message",
    "android-apk-message": "echo Apk location: /android/app/build/outputs/apk/",
    "android": "npm run node-scripts && react-native run-android",
    "ios": "npm run node-scripts && react-native run-ios",
    "start": "npm run node-scripts && react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@trustpayments/jwt-generator": "0.1.21",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-webview": "11.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/runtime": "7.12.13",
    "@react-native-community/eslint-config": "1.1.0",
    "@types/jest": "25.2.3",
    "@types/react-native": "0.63.2",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "babel-jest": "25.1.0",
    "eslint": "6.5.1",
    "jest": "25.1.0",
    "metro-react-native-babel-preset": "0.59.0",
    "react-test-renderer": "16.13.1",
    "typescript": "4.1.5"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
